          </Field>
        </Data>
      </DataSource>
      <DataSource id="communityTestimonial" kind="static">
        <Contract>
          <Field name="items" path="items" description="Binding for newsletter-community.testimonials"/>
        </Contract>
        <Data>
          <Field name="items">
            <Array>
              <Object>
                <Field name="quote">
                  <Value>The community wire captures the nuance you never get from product launch notes.</Value>
                </Field>
                <Field name="author">
                  <Value>Akiko Tan</Value>
                </Field>
                <Field name="role">
                  <Value>Head of Enablement, Stripe Atlas</Value>
                </Field>
              </Object>
            </Array>
          </Field>
        </Data>
      </DataSource>
      <DataSource id="communityForm" kind="static">
        <Contract>
          <Field name="value" path="$" description="Binding for newsletter-community.form"/>
        </Contract>
        <Data>
          <Field name="placeholder">
            <Value>community@company.com</Value>
          </Field>
          <Field name="buttonLabel">
            <Value>Join the wire</Value>
          </Field>
          <Field name="disclaimer">
            <Value>Delivered Thursdays. Includes invites to live sessions.</Value>
          </Field>
          <Field name="url">
            <Value>/newsletter/community-wire/signup</Value>
