<?xml version="1.0" encoding="UTF-8"?>
<PageDefinition kind="blueprint" module="ui-blocks" route="newsletter/community" schemaVersion="1.0.0" contentVersion="1.0.0" locale="en-US">
  <Page id="ui-blocks-newsletter-community">
    <Title>UI Block: Newsletter community hub</Title>
    <Regions>
      <Region id="newsletter">
        <Component id="newsletter-community" type="NewsletterSection">
          <Props>
            <Prop name="variant" kind="static">community</Prop>
            <Prop name="badge" kind="static">Community wire</Prop>
            <Prop name="headline" kind="static">Stay close to the builders</Prop>
            <Prop name="description" kind="static">A weekly recap from 5,000+ metadata practitioners sharing what worked, what broke, and how they fixed it.</Prop>
            <Prop name="highlights" kind="binding" contract="communityHighlights.items"/>
            <Prop name="gallery" kind="binding" contract="communityGallery.items"/>
            <Prop name="testimonials" kind="binding" contract="communityTestimonial.items"/>
            <Prop name="form" kind="binding" contract="communityForm.value"/>
            <Prop name="highlight" kind="static">Trade playbooks with peers shipping metadata in production.</Prop>
          </Props>
        </Component>
      </Region>
    </Regions>
    <DataSources>
      <DataSource id="communityHighlights" kind="static">
        <Contract>
          <Field name="items" path="items" description="Binding for newsletter-community.highlights"/>
        </Contract>
        <Data>
          <Field name="items">
            <Array>
              <Object>
                <Field name="title">
                  <Value>Live feedback threads</Value>
                </Field>
                <Field name="description">
                  <Value>Jump into the weekly discussion with product, revenue, and ops leads.</Value>
                </Field>
              </Object>
              <Object>
                <Field name="title">
                  <Value>Templates on demand</Value>
                </Field>
                <Field name="description">
                  <Value>Request workflows and data models â€” other members share what shipped.</Value>
                </Field>
              </Object>
              <Object>
                <Field name="title">
                  <Value>Small group meetups</Value>
                </Field>
                <Field name="description">
                  <Value>Join pods by industry to compare metadata maturity and dashboards.</Value>
                </Field>
              </Object>
            </Array>
          </Field>
        </Data>
      </DataSource>
      <DataSource id="communityGallery" kind="static">
